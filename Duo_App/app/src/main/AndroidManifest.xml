<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.fhict.fontys.kingsen">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/iconforlaucher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/iconforlaucher"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".GameActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".HomeActivity"
            android:label="@string/title_activity_home"
            android:theme="@style/AppTheme" />
        <activity android:name=".MiniGameActivity" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".MiniGameResultActivity" />
        <activity android:name=".GameExplainActivity"></activity>
    </application>

</manifest>